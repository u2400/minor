<?xml version="1.0"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    <!-- fileprint action-->
    <bean name="/filePrintApply" autowire="byName" scope="prototype" 
    	class="com.ekingstar.eams.misc.fileprint.web.action.FilePrintApplyAction">
    </bean>
    
    <bean name="/filePrintAudit" autowire="byName" scope="prototype" 
	    class="com.ekingstar.eams.misc.fileprint.web.action.FilePrintAuditAction">
    </bean>
    
    <bean name="/filePrintManage" autowire="byName" scope="prototype" 
    	class="com.ekingstar.eams.misc.fileprint.web.action.FilePrintManageAction">
    </bean>
    
    <bean name="/examinationPaper" autowire="byName" scope="prototype"
		class="com.ekingstar.eams.misc.examinationpaper.web.action.ExaminationPaperAction">
	</bean>

    <bean name="/bursaryApply" autowire="byName" scope="prototype"
		class="com.shufe.web.action.std.bursary.ApplyAction">
	</bean>

    <bean name="/bursaryInstructor" autowire="byName" scope="prototype"
		class="com.shufe.web.action.std.bursary.InstructorAction">
	</bean>

    <bean name="/bursaryCollege" autowire="byName" scope="prototype"
		class="com.shufe.web.action.std.bursary.CollegeAction">
	</bean>

    <bean name="/bursarySchool" autowire="byName" scope="prototype"
		class="com.shufe.web.action.std.bursary.SchoolAction">
	</bean>

    <bean name="/bursarySetting" autowire="byName" scope="prototype"
		class="com.shufe.web.action.std.bursary.SettingAction">
	</bean>

    <bean name="/bursaryAward" autowire="byName" scope="prototype"
		class="com.shufe.web.action.std.bursary.AwardAction">
	</bean>

    <bean name="/bursarySubject" autowire="byName" scope="prototype"
		class="com.shufe.web.action.std.bursary.SubjectAction">
	</bean>
</beans>