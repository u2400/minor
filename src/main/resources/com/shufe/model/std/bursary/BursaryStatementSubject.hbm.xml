<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.shufe.model.std.bursary" >
	<class name="BursaryStatementSubject" table="zxj_cszt_t">
		<id name="id" column="ID">
			<generator class="sequence">
				<param name="sequence">SEQ_zxj_cszt_t</param>
			</generator>
		</id>
		<property name="options" column="XX"/>
		<property name="name" column="MC" length="300" not-null="true"/>
		<property name="maxContentLength" not-null="true" column="ZDCJ" />
		<property name="required" not-null="true" column="SFBT" />
	</class>
</hibernate-mapping>