<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.shufe.model.quality.evaluate">
<class name="Option" table="JXZL_PJ_XX_T">
	<cache usage="read-write" region="evaluateCache"/>
	<id name="id" column="ID" unsaved-value="null">
		<generator class="sequence">
			<param name="sequence">SEQ_JXZL_PJ_XX_T</param>
		</generator>
	</id>
	<property name="name" column="MC"/>
	<property name="proportion" column="QZ" length="10"/>
	<many-to-one name="optionGroup" column="XXZID" not-null="true"/>
</class>
</hibernate-mapping>